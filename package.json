{
  "name": "@n8n_io/client-oauth2",
  "version": "1.0.2",
  "description": "Straight-forward execution of OAuth 2.0 flows and authenticated API requests",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint --ext .ts src test",
    "lintfix": "eslint --ext .ts src test --fix",
    "format": "prettier --write src test",
    "test-server-open": "PORT=7357 node test/support/server.js & echo $! > server.pid",
    "test-server-close": "if [ -f server.pid ]; then kill -9 $(cat server.pid); rm server.pid; fi",
    "test-node": "PORT=7357 mocha -R spec --bail -r ts-node/register test/**/*.spec.ts",
    "test": "npm run test-server-open && npm run test-node; npm run test-server-close"
  },
  "engines": {
    "node": ">=16.18"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/n8n-io/client-oauth2.git"
  },
  "keywords": [
    "oauth2",
    "authentication",
    "token"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/n8n-io/client-oauth2/issues"
  },
  "homepage": "https://github.com/n8n-io/client-oauth2",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "body-parser": "^1.20.1",
    "chai": "^4.3.7",
    "cors": "^2.8.5",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "mocha": "^10.2.0",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.6"
  }
}
